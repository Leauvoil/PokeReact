{
  "name": "poke-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "5.0.1",
    "reactstrap": "^8.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "js",
        "jsx"
      ],
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/mouse-events-have-key-events": "off",
      "react/prop-types": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1"
  },
  "precommit": "npm run lint-all",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WildCodeSchool/lyon-0219-react-PokeReact.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WildCodeSchool/lyon-0219-react-PokeReact/issues"
  },
  "homepage": "http://WildCodeSchool.github.io/lyon-0219-react-PokeReact"
}
